local Toggle1 = Section1:CreateToggle("AutoFarm Mobs", Settings.AutoFarmMobs, function(State)
Settings.AutoFarmMobs = State

game:GetService("RunService").Stepped:connect(
    function()
        if Settings.AutoFarmMobs then
            game.Players.LocalPlayer.Character:WaitForChild("Humanoid"):ChangeState(11)
        end
    end
)
spawn(
    function()
        while wait() and Settings.AutoFarmMobs do
            pcall(
                function()
                    repeat
                        wait()
                        if Settings.AutoFarmMobs == false then
                            break
                        end
                        if not game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("ForceField") then
                            if getNearestMob().Humanoid.Health > 10 then wait()
                            game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame =
                                getNearestMob().HumanoidRootPart.CFrame * CFrame.new(0, -7, 0) * CFrame.Angles(90, 0, 0)
                                end 

                            if getNearestMob().Humanoid.Health < 10 then wait()
                                game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame =
                                    getNearestMob().HumanoidRootPart.CFrame * CFrame.new(0, 0, 0) *
                                    CFrame.Angles(0, 0, 0)
                            end
                        end
                    until Settings.AutoFarmMobs == false or getNearestMob().Humanoid.Health == 0 wait()
                    if getNearestMob().Humanoid.Health == 0 then
                        getNearestMob():remove()
                    end
                end
            )
        end
    end
)

spawn(
    function()
        while wait() and Settings.AutoFarmMobs do
            wait(0.2)
            pcall(
                function()
                    CheckMate()
                    Use()
                end
            )
        end
    end
)

end)

if isnetworkowner then
local Toggle1 = Section1:CreateToggle("AutoFarm Mobs Safely", Settings.AutoFarmSafely, function(State)
Settings.AutoFarmSafely = State
game:GetService("RunService").Stepped:connect(
    function()
        if Settings.AutoFarmSafely then
            pcall(
                function()
                    game.Players.LocalPlayer.Character:WaitForChild("Humanoid"):ChangeState(11)
                end
            )
        end
    end
)

spawn(
    function()
        while wait(5) and Settings.AutoFarmSafely do
            pcall(
                function()
                    if getNearestiii() == nil then
                        if Settings.Options == "Matter" then wait()
                            game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame =
                                CFrame.new(math.random(-673, -450), -288, math.random(-648, -450))
                        end
                    end
                    if getNearesti() == nil then
                        if Settings.Options == "Away" then wait()
                            game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame =
                                CFrame.new(0, -5000, 0)
                        end
                    end
                    if getNearesti() == nil then
                        if Settings.Options == "Custom" then wait()
                            game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame =
                                CFrame.new(0, -5000, 0)
                        end
                    end
                end
            )
        end
    end
)

spawn(
    function()
        while wait() and Settings.AutoFarmSafely do wait()
            pcall(
                function()
                    if Settings.Options == "Matter" then
                        wait(Settings.Delay)
                        if isnetworkowner(getNearestiii().PrimaryPart) then
                            getNearestiii().PrimaryPart.CFrame =
                                CFrame.new(math.random(-673, -450), -288, math.random(-648, -450))
                        end
                        if getNearestiii().Humanoid.Health > 10 then
                            game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame =
                                getNearestiii().PrimaryPart.CFrame * CFrame.new(0, -7, 0) * CFrame.Angles(90, 0, 0)
                        end
                        if getNearestiii().Humanoid.Health < 10 then
                            game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame =
                                getNearestiii().PrimaryPart.CFrame * CFrame.new(0, 0, 0) * CFrame.Angles(0, 0, 0)
                        end
                        if getNearestiii().Humanoid.Health == 0 then
                            getNearestiii():Destroy()
                        end
                    end

                    if Settings.Options == "Away" then
                        wait(Settings.Delay)
                        if not isnetworkowner(getNearesti().PrimaryPart) then
                        game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame =
                                getNearesti().PrimaryPart.CFrame * CFrame.new(0, -15, 0) * CFrame.Angles(90, 0, 0)
                            end 
                        if isnetworkowner(getNearesti().PrimaryPart) then
                            wait(Settings.Delay)
                            if getNearesti().Humanoid.Health > 10 then
                        game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame =
                                getNearesti().PrimaryPart.CFrame * CFrame.new(0, -7, 0) * CFrame.Angles(90, 0, 0)
                        end
                            getNearesti().PrimaryPart.CFrame = CFrame.new(10000, 10003, Number)
                            if getNearesti().Humanoid.Health < 10 then
                                game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame =
                                    getNearesti().PrimaryPart.CFrame * CFrame.new(0, 0, 0) * CFrame.Angles(0, 0, 0)
                            end
                        end
                        if getNearesti().Humanoid.Health == 0 then
                            getNearesti():Destroy()
                        end
                    end

                    if Settings.Options == "Custom" then
                        wait(Settings.Delay)
                        if getNearestCustom().Humanoid.Health > 10 then
                            game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame =
                                getNearestCustom().PrimaryPart.CFrame * CFrame.new(0, -8, 0) * CFrame.Angles(90, 0, 0)
                        end
                        if isnetworkowner(getNearestCustom().PrimaryPart) then
                            getNearestCustom().PrimaryPart.Position =
                                Vector3.new(Settings.Position1, Settings.Position2, Settings.Position3)
                            if getNearestCustom().Humanoid.Health < 10 then
                                game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame =
                                    getNearestCustom().PrimaryPart.CFrame * CFrame.new(0, 0, 0) * CFrame.Angles(0, 0, 0)
                            end
                        end
                        if getNearestCustom().Humanoid.Health == 0 then
                            getNearestCustom():Destroy()
                        end
                    end
                end
            )
        end
    end
)

spawn(
    function()
        while wait() and Settings.AutoFarmSafely do
            wait(0.2)
            pcall(
                function()
                    CheckMate()
                    Use()
                end
            )
        end
    end
)

end)


for i,v in pairs(game:GetService("ReplicatedStorage").funs:GetChildren()) do
local OldNameCall = nil
OldNameCall =
    hookmetamethod(
    game,
    "__namecall",
    function(self, ...)
        local Args = {...}
        if self.Name == v.Name or self.Name == "maxstat" then
            return nil
        end
        return OldNameCall(self, unpack(Args))
    end
)
end 


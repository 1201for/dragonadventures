Mobs = {"Demon Slayer", "Strong Demon Slayer","Demon","Strong Demon","Fast Demon","Mukago"}




local String = game:GetService("Workspace").npc["you are one rude person"]

local Strong = game:GetService("Workspace").npc["you are one rude person"]
for i, v in pairs(Strong:GetChildren()) do
    if not v:FindFirstChildOfClass("Folder") then
        v.Name = ""
        v:Destroy()
    elseif v.Name ~= "" then
        v.Name = "Mobs"
        v:FindFirstChildOfClass("Folder").Name = "Mobs"
    end
end
if game:GetService("Workspace"):FindFirstChild("chatnpcs")  then
    for i, v in pairs(game:GetService("Workspace").chatnpcs.Quest:GetChildren()) do
        if v:IsA("Model") then
            pcall(function()
            v.Parent = game:GetService("Workspace").chatnpcs
            end)
        end
    end
end

local function GetAwayAssHats()
    local TargetDistance = math.huge
    local Target
    for i, v in ipairs(game.Players:GetPlayers()) do
        if v ~= game.Players.LocalPlayer and v.Character and v.Character:FindFirstChild("HumanoidRootPart") then
            local Mag =
                (game.Players.LocalPlayer.Character.HumanoidRootPart.Position -
                v.Character:FindFirstChild("HumanoidRootPart").Position).Magnitude
            if Mag < TargetDistance then
                TargetDistance = Mag
                Target = v
            end
        end
    end
    return Target
end



local function getNearestMobFinalSeclection()
    local TargetDistance = math.huge
    local Target
    for i, v in ipairs(String.Mobs.Mobs:GetChildren()) do
        if
            v.Name == Settings.Mobs and v:IsA("Model") and v:FindFirstChild("HumanoidRootPart") and
                v:FindFirstChildOfClass("Humanoid").Health ~= 0 
         then
            local Mag =
                (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.PrimaryPart.Position).Magnitude

            if Mag < TargetDistance then
                TargetDistance = Mag
                Target = v
            end
        end
    end
    return Target
end

local function getNearestMob()
    local TargetDistance = math.huge
    local Target
    for i, v in ipairs(String.Mobs.Mobs:GetChildren()) do
        if
            v.Name == Settings.Mobs and v:IsA("Model") and v:FindFirstChild("HumanoidRootPart") and
                v:FindFirstChildOfClass("Humanoid").Health ~= 0 and
                (GetAwayAssHats().Character.HumanoidRootPart.Position - v.PrimaryPart.Position).Magnitude >= 200 
         then
            local Mag =
                (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.PrimaryPart.Position).Magnitude

            if Mag < TargetDistance then
                TargetDistance = Mag
                Target = v
            end
        end
    end
    return Target
end

local function getNearestiii()
    local TargetDistance = math.huge
    local Target
    for i, v in ipairs(String.Mobs.Mobs:GetChildren()) do
        if
            v.Name == Settings.Mobs and v:IsA("Model") and v:FindFirstChild("HumanoidRootPart") and
                v:FindFirstChildOfClass("Humanoid").Health ~= 0 and
                (GetAwayAssHats().Character.HumanoidRootPart.Position - v.PrimaryPart.Position).Magnitude >= 200
         then
            local Mag = (Vector3.new(-673, -288, -648) - v.PrimaryPart.Position).Magnitude
            if Mag < TargetDistance then
                TargetDistance = Mag
                Target = v
            end
        end
    end
    return Target
end

local function getNearestCustom()
    local TargetDistance = math.huge
    local Target
    for i, v in ipairs(String.Mobs.Mobs:GetChildren()) do
        if
            v.Name == Settings.Mobs and v:IsA("Model") and v:FindFirstChild("HumanoidRootPart") and
                v:FindFirstChildOfClass("Humanoid").Health ~= 0 and
                (GetAwayAssHats().Character.HumanoidRootPart.Position - v.PrimaryPart.Position).Magnitude >= 200
         then
            local Mag =
                (Vector3.new(Settings.Position1, Settings.Position2, Settings.Position3) - v.PrimaryPart.Position).Magnitude
            if Mag < TargetDistance then
                TargetDistance = Mag
                Target = v
            end
        end
    end
    return Target
end

local function getNearesti()
    local TargetDistance = math.huge
    local Target
    for i, v in ipairs(String.Mobs.Mobs:GetChildren()) do
        if
            v.Name == Settings.Mobs and v:IsA("Model") and v:FindFirstChild("HumanoidRootPart") and
                v:FindFirstChildOfClass("Humanoid").Health ~= 0 and
                (GetAwayAssHats().Character.HumanoidRootPart.Position - v.PrimaryPart.Position).Magnitude >= 200
         then
            local Mag = (Vector3.new(10000, 10000, 0) - v.PrimaryPart.Position).Magnitude
            if Mag < TargetDistance then
                TargetDistance = Mag
                Target = v
            end
        end
    end
    return Target
end

local Number = math.random(1,500)

Areas = {}
for i, v in pairs(game:GetService("ReplicatedStorage").Areas:GetChildren()) do
    if v:IsA("BasePart") then
        table.insert(Areas, v.Name)
    end
end
if game:GetService("Workspace"):FindFirstChild("chatnpcs") then
    Que = {}
    for i, v in pairs(game:GetService("Workspace").chatnpcs:GetChildren()) do
        if v:FindFirstChild("Chat") and v.Chat:FindFirstChild("Start") and v.Chat.Start:FindFirstChild("b") and 
            v.Chat.Start.b:FindFirstChild("Quest")  and
                not table.find(Que, v.Chat.Start.b.Quest.Value)
         then
            table.insert(Que, v.Chat.Start.b.Quest.Value)
        end
    end
    Areas = {}
    for i, v in pairs(game:GetService("ReplicatedStorage").Areas:GetChildren()) do
        if v:IsA("BasePart") then
            table.insert(Areas, v.Name)
        end
    end

    Breathings1 = {}
    for i, v in pairs(game:GetService("Workspace").chatnpcs.breath:GetChildren()) do
        if v:IsA("Model") then
            table.insert(Breathings1, v.Name)
        end
    end
    NPCS = {}
    for i, v in pairs(game:GetService("Workspace").chatnpcs:GetChildren()) do
        if v:IsA("Model") then
            table.insert(NPCS, v.Name)
        end
    end
end




local function Esp(v)
     if v:WaitForChild("Humanoid") then
        v:WaitForChild("Humanoid").HealthDisplayDistance = 100
        v:WaitForChild("Humanoid").HealthDisplayType = Enum.HumanoidHealthDisplayType.AlwaysOn
    end
end
spawn(
    function()
        while wait(5) do
            pcall(
                function()
                    for _, v in next, game:GetService("Players"):GetPlayers() do
                        if v.Character ~= nil then
                            Esp(v.Character)
                        end
                    end
                    for i,v in pairs(String.Mobs.Mobs:GetChildren()) do
                        if v:IsA("Model") and v:FindFirstChild("Humanoid") then
                            Esp(v)
                        end
                    end 
                end
            )
        end
    end
)

local function CheckMate() 
    if not game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("ForceField") then
    if game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Katana") then
        wait()
        game:GetService("Players").LocalPlayer.Character:WaitForChild("Humanoid"):EquipTool(
            game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Katana")
        )
    end
    if not game:GetService("Players").LocalPlayer.states:FindFirstChild("exeing") then
        game:GetService("VirtualInputManager"):SendKeyEvent(true, "B", false, game)
    end
    if not game:GetService("Players").LocalPlayer.states:FindFirstChild("selfstun") and game:GetService("Players").LocalPlayer.CharStats.Race.Value == "Human" then
        game:GetService("VirtualInputManager"):SendKeyEvent(true, "R", false, game)
    end
    wait()
    game:GetService("VirtualUser"):ClickButton1(Vector2.new(0, 300))
    game:GetService("ReplicatedStorage").events.remote:FireServer("NormalAttack")
    end 
end

local function Use()
    for i, v in pairs(game:GetService("Players").LocalPlayer.Backpack:GetChildren()) do
        if
            v:IsA("Tool") and v:FindFirstChild("Move") and not v:FindFirstChild("Cash") and
                not game:GetService("Players").LocalPlayer.cds:FindFirstChild(v.Name)
         then wait()
            if game:GetService("Players").LocalPlayer.NoSaveValue.Air.Value >= 100 then
                game:GetService("ReplicatedStorage").events.remote:FireServer(v.Name)
            end
        end
    end
end



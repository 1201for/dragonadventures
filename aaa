local Toggle1 = Section1:CreateToggle("Auto Accept Quest", Settings.AutoQuest, function(State)
Settings.AutoQuest = State
game:GetService("RunService").Stepped:connect(
    function()
        if Settings.AutoQuest then
            game.Players.LocalPlayer.Character:WaitForChild("Humanoid"):ChangeState(11)
        end
    end
)
spawn(
    function()
        while Settings.AutoQuest do
            wait()
            if
                not game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("dialogGUI") and
                    not game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("Quest")
             then
                for i, v in pairs(game:GetService("Workspace").chatnpcs:GetChildren()) do
            if v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Chat") and v.Chat:FindFirstChild("Start") and v.Chat.Start:FindFirstChild("b") and 
            v.Chat.Start.b:FindFirstChild("Quest")  and
                            v:WaitForChild("Chat"):WaitForChild("Start"):WaitForChild("b"):WaitForChild("Quest").Value == Settings.Quest
                     then
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame *CFrame.new(0,-26,0)
                        fireclickdetector(v:WaitForChild("JumpHitbox").ClickDetector)
                    end
                end
            end 
            if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("dialogGUI") then
                game:GetService("VirtualUser"):ClickButton1(Vector2.new(900, 900))
                for i, v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.dialogGUI.f.sf:GetChildren()) do
                    if v:IsA("UIGridLayout") then
                        v.CellSize = UDim2.new(500, 500, -500, -500)
                    end
                    wait(0.1)
                    if v.Name == "button" then
                        v:remove()
                    end
                end
            end
        end
    end
)


end)



                    Player.Character:WaitForChild("Humanoid"):ChangeState(11)
                    for i, v in pairs(Player.Backpack:GetChildren()) do
                        if v:IsA("Tool") and v:FindFirstChild("SwordName") and Settings.FightMethod == "Sword" then
                            v.Parent = Player.Character
                        elseif v:IsA("Tool") and v.Name:find("Combat") and Settings.FightMethod == "Fist" then
                            v.Parent = Player.Character
                        end
                    end
                end
            )
        end
    end
)

spawn(
    function()
